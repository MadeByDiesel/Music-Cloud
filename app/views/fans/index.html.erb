<div class='container' id="main">
  <div class='row'>
    <h1>Listing fans</h1>

    <table class='table'>
      <thead>
        <tr>
          <th>Fan Name</th>
          <th>City</th>
          <th>Country</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @fans.each do |fan| %>
          <tr>
            <td><%= link_to fan.fan_name, fan %></td>
            <td><%= fan.city %></td>
            <td><%= fan.country %></td>
            <td><%= link_to 'Show', fan %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>