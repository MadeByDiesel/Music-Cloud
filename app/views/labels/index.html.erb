<div class='container' id="main">
  <div class='row'>
    <h1>Listing labels</h1>

    <table class='table'>
      <thead>
        <tr>
          <th>Labelname</th>
          <th>City</th>
          <th>Country</th>
          <th>Contactname</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @labels.each do |label| %>
          <tr>
            <td><%= link_to label.label_name, label %></td>
            <td><%= label.city %></td>
            <td><%= label.country %></td>
            <td><%= label.contact_name %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>